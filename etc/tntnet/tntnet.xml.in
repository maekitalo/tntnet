<?xml version="1.0" encoding="UTF-8"?>
<tntnet>
  <mappings>
    <mapurl>
      <target>redirect@tntnet</target>
      <url>^/demos/controls$</url>
      <pathinfo>/demos/controls/</pathinfo>
    </mapurl>
    <mapurl>
      <target>$1@controls</target>
      <url>^/demos/controls/(.+)\..*</url>
    </mapurl>
    <mapurl>
      <target>index@controls</target>
      <url>^/demos/controls/$</url>
      <pathinfo>index</pathinfo>
    </mapurl>
    <mapurl>
      <target>index@controls</target>
      <url>^/demos/controls/(.+)</url>
      <pathinfo>$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>$1@$1</target>
      <url>^/demos/(.+)\..*</url>
      <pathinfo>$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>$1@$1</target>
      <url>^/demos/(.+)</url>
    </mapurl>
    <mapurl>
      <target>tntnet@hello</target>
      <url>^/demos/tntnet.jpg</url>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>(.*)/$</url>
      <pathinfo>/$1/index.html</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/(.*)$</url>
      <pathinfo>/$1</pathinfo>
    </mapurl>
  </mappings>

  <listeners>
    <listener>
      <ip></ip>
      <port>80</port>
    </listener>
  </listeners>

  <daemon>1</daemon>
  <user>www-data</user>
  <group>www-data</group>
  <pidFile>@localstatedir@/run/tntnet.pid</pidFile>
  <documentRoot>/var/www</documentRoot>
  <compPath>
    <entry>@libdir@/tntnet</entry>
  </compPath>
  <logging>
    <rootlogger>INFO</rootlogger>
    <loggers>
      <logger>
        <category>tntnet</category>
        <level>INFO</level>
      </logger>
    </loggers>
    <file>/var/log/tntnet/tntnet.log</file>
    <maxfilesize>1MB</maxfilesize>
    <maxbackupindex>2</maxbackupindex>
    <!-- <host>localhost:1234</host> --> <!--  # send log-messages with udp -->
  </logging>
</tntnet>
